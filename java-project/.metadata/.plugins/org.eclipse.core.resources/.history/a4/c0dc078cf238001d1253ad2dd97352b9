package ch01;

// VarEx05 -> main

// Note -> X

class Note {
	int price = 2000;
}

public class VarEx05 {

	public static void main(String[] args) {
		new Note(); // heap 공간에 Note 클래스가 가지고 있는 모든 데이터를 할당해!! (대신 static 제외해)
		new Note();
		new Note(); // heap 공간
		int age = 25; // main stack 공간
	}

}
